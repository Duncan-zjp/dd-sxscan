.all_main{display: flex;justify-content: center;align-items: center;padding-top: 15px;background: #fff}
form{display: flex;justify-content: center;width: 100%;flex-wrap: wrap;position: relative;bottom: 15%}
.all_main .header_img image{width: 80px;height:80px;background-size: 100%;}
.box{display: flex;align-items: center;padding:10px 0px 10px;flex: 0 0 100%;justify-content: center}
.box input{border-bottom: 1px solid #333;}
.box text{padding-right: 15px;font-weight: 700;font-size: 16px;}

.box .dl{width: 161px;height: 38px;display: flex;align-items: center;justify-content: center;background: #09bcfe;font-size: 13px;}
.anniu {margin-top: 30px;justify-content: center;}